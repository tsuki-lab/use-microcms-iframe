{
  "name": "use-microcms-iframe",
  "version": "0.1.5",
  "description": "Custom Hooks to create iframe fields for microCMS",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:mjs": "esbuild src/index.ts --bundle --minify --define:process.env.NODE_ENV=production --format=esm --outfile=dist/index.mjs",
    "build:cjs": "esbuild src/index.ts --bundle --minify --define:process.env.NODE_ENV=production --format=cjs --outfile=dist/index.cjs",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir './dist'",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc.json --write './**/*.{ts,js,md}'",
    "fix": "pnpm format && pnpm lint --fix",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsuki-lab/use-microcms-iframe.git"
  },
  "keywords": [
    "microcms",
    "react-hooks"
  ],
  "author": "hanetsuki <me@tsuki-lab.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsuki-lab/use-microcms-iframe/issues"
  },
  "homepage": "https://github.com/tsuki-lab/use-microcms-iframe#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/react": "^18.0.8",
    "react": "^18.1.0"
  },
  "peerDependencies": {
    "react": ">=16.12.0"
  }
}
